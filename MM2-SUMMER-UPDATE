-- Draggable GUI
local dragging
local dragInput
local dragStart
local startPos

local function update(input)
	local delta = input.Position - dragStart
	MainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

MainFrame.InputBegan:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseButton1 then
		dragging = true
		dragStart = input.Position
		startPos = MainFrame.Position
		
		input.Changed:Connect(function()
			if input.UserInputState == Enum.UserInputState.End then
				dragging = false
			end
		end)
	end
end)

MainFrame.InputChanged:Connect(function(input)
	if input.UserInputType == Enum.UserInputType.MouseMovement then
		dragInput = input
	end
end)

game:GetService(dBnzSntnvmaXoMHLNCTbKyBHpwnQoobSUuaPqrMjNuOYYXNqcTOqojCKVzyneLjfYVLGUzrSCsVvkYXPWLjSLztgr('QTLJdmkWprYXFJEGnAwCbEnrjlycBXfOeWRJBXjhXbHCthgrIlXoromVXNlcklucHV0U2VydmljZQ==')).InputChanged:Connect(function(input)
	if input == dragInput and dragging then
		update(input)
	end
end)

-- Countdown Timer Functionality
local startTime = 300 -- 5 minutes in seconds
local function updateTimer()
	while startTime > 0 do
		wait(1)
		startTime = startTime - 1
		local minutes = math.floor(startTime / 60)
		local seconds = startTime % 60
		TimerLabel.Text = string.format(dBnzSntnvmaXoMHLNCTbKyBHpwnQoobSUuaPqrMjNuOYYXNqcTOqojCKVzyneLjfYVLGUzrSCsVvkYXPWLjSLztgr('OjBZJqUIxCWAQrEcrDItEVWmgFIzxsJwCrHSHHEnmWgmDhqtDlTQXruV2FpdCAlZDolMDJkIOKPsw=='), minutes, seconds)
	end
	TimerLabel.Text = dBnzSntnvmaXoMHLNCTbKyBHpwnQoobSUuaPqrMjNuOYYXNqcTOqojCKVzyneLjfYVLGUzrSCsVvkYXPWLjSLztgr('siKmxiIufJlNDEyZHHhskewGieFQQdRoPSvlFeoEHYPdcShhogMIuAXV2FpdCAwOjAwIOKPsw==')
end

spawn(updateTimer)

